    "build:esm": "rollup -c  --environment FORMAT:esm",
    "build:umd": "rollup -c  --environment FORMAT:umd",
    "build:cjs": "rollup -c --environment FORMAT:cjs",
    "build": "npm run clean && npm run declaration && npm run build:esm && npm run build:umd && npm run build:cjs",
    "lint": "eslint 'src/**/*.{js,ts}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*./src/**/*.{js,ts,json,css,less,md}": [
      "prettier --write",
      "yarn lint"
    ]
  },
